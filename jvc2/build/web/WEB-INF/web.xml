<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>updateservlet</servlet-name>
        <servlet-class>updateservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addservlet</servlet-name>
        <servlet-class>addservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>assignservlet</servlet-name>
        <servlet-class>assignservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateassignservlet</servlet-name>
        <servlet-class>updateassignservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveentry</servlet-name>
        <servlet-class>saveentry</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateentryservlet</servlet-name>
        <servlet-class>updateentryservlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>effortservlet</servlet-name>
        <servlet-class>effortservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateservlet</servlet-name>
        <url-pattern>/updateservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addservlet</servlet-name>
        <url-pattern>/addservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>assignservlet</servlet-name>
        <url-pattern>/assignservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateassignservlet</servlet-name>
        <url-pattern>/updateassignservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveentry</servlet-name>
        <url-pattern>/saveentry</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateentryservlet</servlet-name>
        <url-pattern>/updateentryservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>effortservlet</servlet-name>
        <url-pattern>/effortservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
